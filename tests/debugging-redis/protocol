=== LEGEND ====================================================================
s : start
p : project name
x : project id
d : disassemble
b : breakpoints
w : watchpoints
i : request id
c : command
r : result
o : stdout
e : stderr
f : fail



=== start =====================================================================
<--
{
  "s" : "<projectid>",
  "i" : "<some_id>"
}

-->
{
  "s" : "<projectid>",
  "i" : "<some_id>"
}



=== disassemble ===============================================================
<--
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "d" : ["<func_1>", ... , "<func_n>"]
}

-->
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "d" : {
          "<func_1>" : "<result_1>",
          ...
          "<func_n>" : "<result_n>"
        }
}



=== breakpoints ===============================================================
<--
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "b" : ["<bp_1>", ... , "<bp_n>"]
}

-->
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "b" : {
          "<bp_1>" : "<result_1>",
          ...
          "<bp_n>" : "<result_n>"
        }
}

NOTE: <bp_n> must be either function name or number represented as string



=== watchpoints ===============================================================
<--
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "w" : ["<wp_1>", ... , "<wp_n>"]
}

-->
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "w" : {
          "<wp_1>" : "<result_1>",
          ...
          "<wp_n>" : "<result_n>"
        }
}



=== commands ==================================================================
<--
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "c" : "<run / step / next / continue / info locals / backtrace / quit>"
}

-->
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "r" : "<command_result>",
  "o" : "<program_output>",
  "e" : "<program_error>"
}
