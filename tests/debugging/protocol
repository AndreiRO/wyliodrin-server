=== disassemble ===============================================================
<--
{
  "p" : "<projectid>",
  "d" : ["<func_1>", ... , "<func_n>"]
}

-->
{
  "p" : "<projectid>",
  "d" : {
          "<func_1>": "<result_1>",
          ...
          "<func_n>": "<result_n>"
        }
}



=== breakpoints ===============================================================
<--
{
  "p" : "<projectid>",
  "b" : ["<bp_1>", ... , "<bp_n>"]
}

<--
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "c" : "<step or next>"
}

-->
{
  "p" : "<projectid>",
  "i" : "<some_id>",
  "r" : "<command_result>",
  "o" : "<program_output>",
  "e" : "<program_error>"
}



=== watchpoints ===============================================================
<--
{
  "p" : "<projectid>",
  "w" : ["<wp_1>", ... , "<wp_n>"]
}



=== LEGEND ====================================================================
p : project id
d : disassemble
b : breakpoints
i : request id
c : command
r : result
o : stdout
e : stderr
w : watchpoints
