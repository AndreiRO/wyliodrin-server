=== disassemble ===============================================================
<--
{
  "project"          : "<projectid>",
  "disassemble_func" : ["<func_1>", ... , "<func_n>"]
}

-->
{
  "project"           : "<projectid>",
  "disassemble_func"  : {
                          "<func_1>": "<result_1>",
                          ...
                          "<func_n>": "<result_n>"
                        }
}



=== breakpoints ===============================================================
<--
{
  "project"     : "<projectid>",
  "breakpoints" : ["<bp_1>", ... , "<bp_n>"]
}

<--
{
  "project" : "<projectid>",
  "id"      : "<some_id>",
  "command" : "<step or next>"
}

-->
{
  "project" : "<projectid>",
  "id"      : "<some_id>",
  "result"  : "<command_result>",
  "stdout"  : "<program_output>",
  "stderr"  : "<program_error>"
}



=== watchpoints ===============================================================
<--
{
  "project" : "<projectid>",
  "watch"   : ["<wp_1>", ... , "<wp_n>"]
}
